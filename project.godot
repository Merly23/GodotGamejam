; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Character",
"language": "GDScript",
"path": "res://source/character/Character.gd"
}, {
"base": "Node",
"class": "FiniteStateMachine",
"language": "GDScript",
"path": "res://addons/finite_state_machine/source/FiniteStateMachine.gd"
}, {
"base": "Camera2D",
"class": "GameCam",
"language": "GDScript",
"path": "res://source/camera/GameCam.gd"
}, {
"base": "CanvasLayer",
"class": "Interface",
"language": "GDScript",
"path": "res://source/interface/hud/Interface.gd"
}, {
"base": "Node",
"class": "SceneLoader",
"language": "GDScript",
"path": "res://source/tools/SceneLoader.gd"
}, {
"base": "Node",
"class": "ScreenShake",
"language": "GDScript",
"path": "res://source/camera/ScreenShake.gd"
}, {
"base": "Node",
"class": "State",
